<?xml version="1.0" encoding="UTF-8"?>

<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
                   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
         http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.2.xsd">

  <!-- Vetstoria schema table definition -->
  <changeSet id="1" author="Pamoda" logicalFilePath="src/main/resources/changelog_vetstoria.xml">
    <!-- vt_user -->
    <createTable tableName="vt_user">
        <column name="user_id" type="number(10)">
          <constraints primaryKey="true" nullable="false" primaryKeyName="pk_user_id" />
        </column>
        <column name="user_name" type="text" />
    </createTable>
    <createSequence incrementBy="1" sequenceName="seq_user_id" />
  </changeSet>
            
</databaseChangeLog>
